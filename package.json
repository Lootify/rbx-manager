  "bugs": {
    "url": "https://github.com/zachariapopcorn/rbx-manager/issues"
  },
  "homepage": "https://github.com/zachariapopcorn/rbx-manager#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "funcaptcha": "^1.1.7",
    "ms": "^2.1.3",
    "noblox.js": "^4.14.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/ms": "^0.7.31"
